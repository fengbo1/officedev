<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="import_default" extends="struts-default" >
		<action name="importcshsj" class="office.importcsh.ImportCshsj">
			<result name="success">/page/public/success.jsp</result>
			<result name="failed">/page/public/error.jsp</result>
		</action>
		<action name="importpic" class="office.importcsh.ImportPic">
			<result name="success">/page/public/success.jsp</result>
			<result name="failed">/page/public/error.jsp</result>
		</action>
		<action name="displaypic" class="office.importcsh.DisplayPic">
			<result name="success">/page/import/display.jsp</result>
		</action>
	</package>
</struts>    
